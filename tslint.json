{
  "extends": [
    "tslint-react",
    "tslint-config-prettier"
  ],
  "rules": {
    "jsx-wrap-multiline": false,
    "jsx-boolean-value": false,
    "jsx-no-bind": false,
    "jsx-self-close": false,
    "jsx-no-lambda": false
  },
  "linterOptions": {
    "exclude": [
      "config/**/*.js",
      "node_modules/**/*.ts"
    ]
  }
}